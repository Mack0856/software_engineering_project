<h1>Users</h1>
<table class="table table-striped table-bordered" cellspacing="0" width="100%">
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Email</th>
      <th>Permissions</th>
      <th>Total Number of Songs played</th>
      <th>Total Number of Songs downloaded</th>
      <th>Next Payment On</th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.first_name %></td>
        <td><%= user.last_name %></td>
        <td><%= user.email %></td>
        <td><%= user.permissions %></td>
        <td><%= user.total_songs_played %></td>
        <td><%= user.total_songs_downloaded %></td>
        <td><%= user.next_payment_on %></td>
      </tr>
    <% end %>
  </tbody>
</table>